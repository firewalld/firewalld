<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/org/fedoraproject/FirewallConfig/" id="org.fedoraproject.FirewallConfig">
    <key type="b" name="show-ipsets">
      <default>true</default>
      <summary>Shows IPSets tab if true</summary>
    </key>
    <key type="b" name="show-icmp-types">
      <default>false</default>
      <summary>Shows ICMP types tab if true</summary>
    </key>
    <key type="b" name="show-direct">
      <default>false</default>
      <summary>Shows direct chains and rules tab if true</summary>
    </key>
    <key type="b" name="show-lockdown-whitelist">
      <default>false</default>
      <summary>Shows lockdown whitelist tab if true</summary>
    </key>
    <key type="b" name="show-helpers">
      <default>false</default>
      <summary>Shows Helpers tab if true</summary>
    </key>
    <key type="b" name="show-active-bindings">
      <default>true</default>
      <summary>Shows active zone bindings if true</summary>
    </key>
  </schema>
</schemalist>
